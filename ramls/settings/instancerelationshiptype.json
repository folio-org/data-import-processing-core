{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "A qualifier indicating the type of relationship between two Instance records",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "description": "label for the relationship type",
      "type": "string"
    },
    "metadata": {
      "type": "object",
      "$ref": "../raml-storage/raml-util/schemas/metadata.schema",
      "readonly": true
     }
  },
  "additionalProperties": true,
  "required": [
    "name"
  ]
}
